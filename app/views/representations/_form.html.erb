<%= form_with(model: @representation, local: true, data: { turbo: false }, url: representations_path) do |form|%>

<div>
  <%= form.label :sity %>
  <%= form.text_field :sity %>
</div>

<div>
  <%= form.label :president %>
  <%= form.text_field :president%>
</div>
  
<div>
  <%= form.label :adress %>
  <%= form.rich_text_area :adress %>
</div>

<div>
  <%= form.label :image %>
  <%= form.file_field :image %>
</div>

<div>
  <%= form.label :pictures %>
  <%= form.file_field :pictures, multiple: true %>
</div>

<%= form.submit%>

<% end %>